{% extends '_base.html.twig' %}

{% block title %}my profile{% endblock %}

{% block main %}
    <form action="/update" method="post">
        <label>Username:</label><input type="text" name="username" value="{{ username }}"><br>
        <label>Id:</label><input type="text" name="id" value="{{ userProfile.id }}"><br>
        <label>Role:</label><input type="text" name="role" value="{{ userProfile.role }}"><br>
        <label>Email:</label><input type="text" name="email" value="{{ userProfile.email }}"><br>
        <input type="submit" value="Update">
    </form>
{% endblock %}